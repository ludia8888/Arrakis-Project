# Development Environment Variables for All Services

# JWT Keys
JWT_PRIVATE_KEY_

JWT_PUBLIC_KEY_

JWT_SECRET_KEY=your-development-secret-key-here
JWT_ALGORITHM=RS256
JWT_ISSUER=user-service
JWT_AUDIENCE=oms

# Service URLs
USER_SERVICE_URL=http://localhost:8002
OMS_SERVICE_URL=http://localhost:8000
AUDIT_SERVICE_URL=http://localhost:8001
IAM_SERVICE_URL=http://localhost:8003

# Database URLs
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/user_db
TERMINUS_SERVER=http://localhost:6363
TERMINUS_DB=ontology_db
TERMINUS_USER=admin
TERMINUS_PASSWORD=root

# Redis
REDIS_URL=redis://localhost:6379

# Development Settings
ENVIRONMENT=development
DEBUG=true
TZ=UTC

# NATS
NATS_URL=nats://localhost:4222

# Service Discovery
ENABLE_SERVICE_DISCOVERY=true
SERVICE_NAME=development